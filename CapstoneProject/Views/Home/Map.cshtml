
@{
    ViewBag.Title = "Map";
}
<!DOCTYPE html>
<html>
<head>
    <title>Map</title>
    <meta name="viewport" content="initial-scale=1.0" />
    <meta charset="utf-8" />
    <link rel="stylesheet" href="~/Content/Site.css" />
</head>
<body>
    <div class="container-fluid">
        <div class="row">
            <div class="col">
                <h1 class="display-4">Map</h1>
            </div>
        </div>
        <div class="row">
            <div class="col">
                <div id="map"></div>
            </div>
        </div>
    </div>
    <script>
      var map;
      function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
          center: {lat: -34.397, lng: 150.644},
          zoom: 8
        });
      }
    </script>
    <script src=@ViewBag.MapURL async defer></script>

    </body>
</html>
